{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"version": 2,
	"patterns": [
		"header-mobile-only",
		"sidebar-portfolio",
		"footer-portfolio"
	],
	"settings": {
		"shadow": {
			"presets": [
				{
					"name": "Glow",
					"slug": "glow",
					"shadow": "0 .5em 1em -.125em rgba(10,10,10,.1),0 0 0 1px rgba(10,10,10,.02)"
				}
			]
		}
	},
	"styles": {
		"css": "hr.is-style-wide {width: 100%;}"
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
	]
}
